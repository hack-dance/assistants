{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "strict": false,
  "noEmit": true,
  "allowJs": true,
  "compilerOptions": {
    "lib": [
      "esnext",
      "DOM",
      "es6",
      "dom.iterable"
    ],
    "jsx": "preserve",
    "module": "esnext",
    "target": "ESNext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}
